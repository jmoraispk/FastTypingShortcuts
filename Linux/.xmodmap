keycode 108 = Mode_switch

! Arrows
keysym j = j J Left
keysym l = l L Right
keysym i = i I Up
keysym k = k K Down

! Home and End
keysym u = u U Home
keysym o = o O End

! Delete key
keycode 47 = ccedilla Ccedilla Delete

! Greater and Smaller signs mapped to Alt + , and Alt + .
keycode 59 = comma semicolon less
keycode 60 = period colon greater

! @ for 2, because it sometimes isn't
keycode 11 = 2 quotedbl at

! {} for 7 and 0 keys
keycode 16 = 7 slash braceleft
keycode 19 = 0 equal braceright
